{
  "manifest_version": 2,

  "name": "Swagger-UI",
  "description": "Swagger User Interface",
  "version": "1.0",
"icons": { 
	"16": "images/logo_small.png",
	"128": "images/swagger-round.png"
},
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
"app": {
    "launch": {
      "local_path": "index.html"
    }
  }
}
